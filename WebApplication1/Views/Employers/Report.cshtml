@Styles.Render("~/Content/Theme/assets/layouts/layout/css/layout.min.css")
@Styles.Render("~/Content/Theme/assets/layouts/layout/css/themes/darkblue.min.css")
@Styles.Render("~/Content/Theme/assets/layouts/layout/css/custom.css")


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()


    <body id="BdyLayoutLogin" class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">

        <input id="NameControllerERP" name="NameControllerERP" type="hidden" value="Home">

        <div class="page-wrapper">


            <div class="page-header navbar navbar-fixed-top">
                <div class="page-header-inner ">
                    <div class="top-menu">
                    </div>
                </div>
            </div>

            <div class="clearfix"> </div>
            <div class="page-container">
                <div class="page-content-wrapper">
                    <div style="padding-left:100px; padding-right:100px; min-height:0px;">
                        <div id="BodyApp">
                            <div class="note note-danger bg-red bg-font-red" style="display:none" id="errorFromController">
                                <i class="fa fa-warning"></i>
                                <p class="inline-block" id="strErrorFromController"></p>
                            </div>

                            <div class="erp-box-mn-wrp">

                                <p>
                                    <div class="btn btn-circle green btn-outline btn-border-1x">
                                        @Html.ActionLink("Atrás", "UserManagment")
                                    </div>
                                </p>
                                <!-- END PORTLET TITLE-->
                                <div class="portlet blue box erp-portlet-table" id="ListaEmpleadoTH">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            <i class="fa fa-list"></i>Lista Empleados
                                        </div>
                                        <div class="tools" style="font-size:16px">

                                            <a href="javascript:;" class="font-white" data-original-title="" title="" id="XLSExp">
                                                <i class="fa fa-file-excel-o"></i>
                                            </a>
                                            <a href="javascript:;" class="collapse" data-original-title="" title=""></a>
                                        </div>
                                    </div>
                                    <!-- END PORTLET TITLE-->
                                    <div class="portlet-body ">


                                        <div id="grdEmpleadoTH_wrapper" class="dataTables_wrapper no-footer">
                                            
                                            <div class="dataTables_scroll">
                                             
                                                <div class="dataTables_scrollBody" style="position: relative; overflow: auto; width: 100%;">
                                                    <table class="table table-striped table-bordered table-hover dataTable no-footer erp-table js-dtable-filters 0982c6ec-5e40-4330-90a6-8674ea404893" id="grdEmpleadoTH" width="100%" role="grid" aria-describedby="grdEmpleadoTH_info" style="width: 100%;">
                                                        <thead>
                                                            <tr role="row" style="height: 0px;">                                                              
                                                                <th class="sorting_desc" aria-controls="grdEmpleadoTH" rowspan="1" colspan="1" style="width: 132px; padding-top: 0px; padding-bottom: 0px; border-top-width: 0px; border-bottom-width: 0px; height: 0px;" aria-sort="descending" aria-label=" Activo : activate to sort column ascending">
                                                                    <div class="dataTables_sizing" style="height:0;overflow:hidden;"> Activo </div>
                                                                </th>
                                                                <th class="sorting" aria-controls="grdEmpleadoTH" rowspan="1" colspan="1" style="width: 132px; padding-top: 0px; padding-bottom: 0px; border-top-width: 0px; border-bottom-width: 0px; height: 0px;" aria-label=" Tipo de Documento : activate to sort column ascending">
                                                                    <div class="dataTables_sizing" style="height:0;overflow:hidden;"> Tipo de Documento </div>
                                                                </th>
                                                                <th class="sorting" aria-controls="grdEmpleadoTH" rowspan="1" colspan="1" style="width: 112px; padding-top: 0px; padding-bottom: 0px; border-top-width: 0px; border-bottom-width: 0px; height: 0px;" aria-label=" Documento : activate to sort column ascending">
                                                                    <div class="dataTables_sizing" style="height:0;overflow:hidden;"> Documento </div>
                                                                </th>
                                                                <th class="sorting" aria-controls="grdEmpleadoTH" rowspan="1" colspan="1" style="width: 112px; padding-top: 0px; padding-bottom: 0px; border-top-width: 0px; border-bottom-width: 0px; height: 0px;" aria-label=" Nombre : activate to sort column ascending">
                                                                    <div class="dataTables_sizing" style="height:0;overflow:hidden;"> Nombre </div>
                                                                </th>
                                                                <th class="sorting" aria-controls="grdEmpleadoTH" rowspan="1" colspan="1" style="width: 113px; padding-top: 0px; padding-bottom: 0px; border-top-width: 0px; border-bottom-width: 0px; height: 0px;" aria-label=" Apellido : activate to sort column ascending">
                                                                    <div class="dataTables_sizing" style="height:0;overflow:hidden;"> Apellido </div>
                                                                </th>
                                                                <th class="sorting" aria-controls="grdEmpleadoTH" rowspan="1" colspan="1" style="width: 237px; padding-top: 0px; padding-bottom: 0px; border-top-width: 0px; border-bottom-width: 0px; height: 0px;" aria-label=" Correo Electrónico : activate to sort column ascending">
                                                                    <div class="dataTables_sizing" style="height:0;overflow:hidden;"> Correo Electrónico </div>
                                                                </th>
                                                            </tr>
                                                        

                                                        </thead>

                                                        <tbody>

                                                            @foreach (var item in Model)
                                                            {
                                                                <tr role="row" class="even">
                                                                    
                                                                    <td align="center" valign="middle" class="sorting_1">
                                                                        <div class="" style="">
                                                                            @if (item.Active == true)
                                                                            {
                                                                                <span style="color: white " class="label label-sm label-success">  Activo</span>
                                                                            }
                                                                            else
                                                                            {
                                                                                <span style="color: white " class="label label-sm label-success"> Inactivo</span>
                                                                            }
                                                                        </div>
                                                                    </td>
                                                                    <td align="center" valign="middle">
                                                                        <div class="" style="">
                                                                            <span style="color: black " class="label label-sm "> @Html.DisplayFor(modelItem => item.DocumentType1.DocumentType1)</span>
                                                                        </div>
                                                                    </td>
                                                                    <td align="center" valign="middle">
                                                                        <div class="" style="">
                                                                            <div class="" style="">
                                                                                <span class="" style="">  @Html.DisplayFor(modelItem => item.Document)</span>
                                                                            </div>
                                                                        </div>
                                                                    </td>
                                                                    <td align="center" valign="middle">
                                                                        <div class="" style="">
                                                                            <div class="" style="">
                                                                                <span class="" style="">@Html.DisplayFor(modelItem => item.Name)</span>
                                                                            </div>
                                                                        </div>
                                                                    </td>
                                                                    <td align="center" valign="middle">
                                                                        <div class="" style="">
                                                                            <div class="" style="">
                                                                                <span class="" style=""> @Html.DisplayFor(modelItem => item.LastName)</span>
                                                                            </div>
                                                                        </div>
                                                                    </td>
                                                                    <td align="center" valign="middle">
                                                                        <div class="" style="">
                                                                            <div class="" style="">
                                                                                <span class="" style="">@Html.DisplayFor(modelItem => item.Email)</span>
                                                                            </div>
                                                                        </div>
                                                                    </td>
                                                                </tr>

                                                            }

                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                            <div class="dataTables_info" id="grdEmpleadoTH_info" role="status" aria-live="polite">Mostrando 1 a 10 de 10 registros</div>
                                            <div class="dataTables_paginate paging_bootstrap_full_number" id="grdEmpleadoTH_paginate">
                                                <ul class="pagination" style="visibility: visible;">
                                                    <li class="prev disabled">
                                                        <a href="#" title="First"><i class="fa fa-angle-double-left"></i></a>
                                                    </li>
                                                    <li class="prev disabled">
                                                        <a href="#" title="Prev"><i class="fa fa-angle-left"></i></a>
                                                    </li>
                                                    <li class="active">
                                                        <a href="#">1</a>
                                                    </li>
                                                    <li>
                                                        <a href="#">2</a>
                                                    </li>
                                                    <li>
                                                        <a href="#">3</a>
                                                    </li>
                                                    <li>
                                                        <a href="#">4</a>
                                                    </li>
                                                    <li>
                                                        <a href="#">5</a>
                                                    </li>
                                                    <li class="next">
                                                        <a href="#" title="Next"><i class="fa fa-angle-right"></i></a>
                                                    </li>
                                                    <li class="next">
                                                        <a href="#" title="Last"><i class="fa fa-angle-double-right"></i></a>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>


                                    </div>
                                    <!-- END PORTLET BODY-->

                                </div>
                                <!-- END PORTLET BODY-->
                            </div>
                        </div>
                    </div>
                    <div class="ajax_loader" id="PageLoader" style="display: none;">
                        <i class="loader" id="loader"></i>
                    </div>
                </div>
            </div>
            <div class="page-footer">
            </div>

        </div>




    </body>

}